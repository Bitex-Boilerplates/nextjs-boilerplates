{
  "name": "bitbot-webapp",
  "description": "bitbot webapp",
  "version": "0.1.0",
  "author": "BitexId",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "storybook": "start-storybook -p 5000"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "clsx": "1.1.1",
    "next": "10.1.3",
    "next-i18next": "8.2.0",
    "next-seo": "4.24.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@next/bundle-analyzer": "10.1.3",
    "@storybook/addon-actions": "^6.3.0-alpha.28",
    "@storybook/addon-essentials": "^6.3.0-alpha.28",
    "@storybook/addon-links": "^6.3.0-alpha.28",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.0-alpha.28",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "@zeit/next-source-maps": "0.0.3",
    "add": "2.0.6",
    "autoprefixer": "10.2.5",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.2.2",
    "commitizen": "4.2.3",
    "critters": "0.0.10",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "next-compose-plugins": "2.2.1",
    "postcss": "8.2.10",
    "prettier": "2.2.1",
    "react-docgen-typescript-loader": "3.7.2",
    "tailwindcss": "2.1.1",
    "tailwindcss-rtl": "0.7.3",
    "typescript": "4.2.4",
    "yarn": "1.22.10"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
